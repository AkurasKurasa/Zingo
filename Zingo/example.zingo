FUN update_points_state(points)
    VAR finalpoints = points + 1
    RETURN finalpoints
END

FUN get_position_values(diff)
    VAR positions = []

    FOR i = 0 TO 5 THEN
        VAR res = diff + i
        APPEND(positions, res)
    END

    RETURN positions
END

FUN get_list_values(lst)
    VAR len = LEN(lst)

    FOR i = 0 TO len THEN
        PRINT(lst/i)
    END
END

FUN r(num)
    VAR result = num + 1
    RETURN result
END

VAR inpone = LIST(input_value)
get_list_values(inpone)